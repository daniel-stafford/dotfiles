  "vim.cursorStylePerMode.visual": "underline",
  "vim.cursorStylePerMode.visualline": "underline",
  "vim.cursorStylePerMode.visualblock": "underline",
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "line-thin",
  "vim.sneak": true,
  "vim.ignorecase": false,
  "editor.scrollBeyondLastLine": false,
  "explorer.confirmDelete": false,
  "shellcheck.enable": true,
  "shellcheck.enableQuickFix": true,
  "shellcheck.run": "onType",
  "shellcheck.executablePath": "shellcheck",
  "shellcheck.exclude": [],
  "shellcheck.customArgs": [],
  "shellcheck.ignorePatterns": {},
  "shellcheck.ignoreFileSchemes": ["git", "gitfs"],
  "shellcheck.useWSL": false,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "git.ignoreMissingGitWarning": true,
  "workbench.colorTheme": "Dark++ Italic",
  "typescript.preferences.importModuleSpecifier": "relative",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [".terraform/**/*", "**/.terraform/**/*"]
  },
  "terraform.languageServer": {
    "external": true,
    "args": ["serve"]
  },
  "java.semanticHighlighting.enabled": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "git.autofetch": true,
  "react-native.packager.port": 19001,
  "python.languageServer": "Microsoft",
  "java.configuration.checkProjectSettingsExclusions": false,
  "python.pythonPath": "/usr/local/bin/python3"
}
